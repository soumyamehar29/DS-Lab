Aim: Write a program to sort the given elements using insertion sort technique.

#include<stdio.h>
void main() {
	int a[20], i, n, j, temp, pos;
	printf("Enter value of n : ");
	scanf("%d", &n);
for(i= 0 ;i< n;i++)
	{
		printf("Enter element for a[%d] : ",i);
		scanf("%d",&a[i]);
	}
	printf("Before sorting the elements in the array are\n");
	for(i = 0 ; i<n;i++)
		{
			printf("Value of a[%d] = %d\n",i,a[i]);
		}
				printf("After sorting the elements in the array are\n");
				for(i = 0; i <n;i++)
					{
						for(j = i+1; j< n;j++)
							{
								if(a[i]>a[j])
								{
									temp = a[i];
									a[i] = a[j];
									a[j] = temp;
								}
							}
					}
	for(i = 0 ;i < n; i++)
					{
						printf("Value of a[%d] = %d\n",i, a[i]);
					}
}
